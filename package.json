{
  "name": "@ltonetwork/lto",
  "version": "0.0.0",
  "description": "LTO library to generate key pairs and sign events",
  "scripts": {
    "_build-partial": "node_modules/.bin/browserify raw/index.js raw/*/index.js --im -s lto -u parse-json-bignumber -u bignumber.js -u crypto -u node-fetch -u ts-utils -u ts-api-validator > ./dist/index.partial.js",
    "_build-full": "node_modules/.bin/browserify raw/index.js raw/*/index.js --im -s lto -u crypto -u node-fetch > ./dist/index.js",
    "_build-clear-folder": "rm -rf ./raw && rm -rf ./dist",
    "build": "npm run _build-clear-folder && webpack --mode production",
    "compile": "mkdir -p raw && ./node_modules/.bin/tsc -p ./tsconfig.json --declaration --outDir raw",
    "_terser-partial": "node_modules/.bin/terser ./dist/index.partial.js -c --source-map -o ./dist/index.partial.min.js",
    "_terser-full": "node_modules/.bin/terser ./dist/index.js -c --source-map -o ./dist/index.min.js",
    "terser": "npm run _terser-full && npm run _terser-partial",
    "test": "node_modules/.bin/mocha --require ts-node/register 'test/**/*.spec.ts'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint-fix": "eslint src --ext .ts,.tsx --fix"
  },
  "author": "LTO Network",
  "license": "MIT",
  "outDir": "./dist",
  "main": "./dist/index.js",
  "types": "raw/lto.d.ts",
  "dependencies": {
    "@types/secp256k1": "^4.0.3",
    "axios": "^0.25.0",
    "bignumber.js": "^4.1.0",
    "blakejs": "^1.1.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "ethereum-cryptography": "^0.1.3",
    "js-sha256": "^0.9.0",
    "jsrsasign": "^10.5.1",
    "node-fetch": "^2.2.0",
    "parse-json-bignumber": "0.0.2",
    "secp256k1": "^4.0.2",
    "ts-api-validator": "^2.1.0",
    "ts-utils": "^6.0.4",
    "tweetnacl": "^1.0.3",
    "webpack": "^5.70.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/crypto-js": "^4.1.1",
    "@types/mocha": "^2.2.43",
    "@types/node": "^15.14.9",
    "@types/sinon": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3",
    "requirejs": "^2.3.5",
    "sinon": "^4.5.0",
    "terser": "^5.7.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^5.0.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.9",
    "webpack-cli": "^4.9.2"
  }
}
